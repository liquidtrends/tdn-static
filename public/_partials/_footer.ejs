<!-- Contact Block -->
	<div class="contact-block">
		<div class="container">
			<!-- Contact Form -->
			<div class="col-md-4">
				<h3><i class="fa fa-envelope"></i> Get in touch</h3>
				<form role="form" action="//formspree.io/liquidtrends@gmail.com"
				      method="POST">
				      	<div class="form-group">
				    		<input placeholder="Your name" class="form-control" type="text" name="name">
				    	</div>
				    	<div class="form-group">
				    		<input placeholder="Your email" class="form-control" type="email" name="_replyto">
				    	</div>
				    	<div class="form-group">
				    		<textarea placeholder="Your message" class="form-control" type="textarea" name="message"></textarea>
				    	</div>
				    	<button type="submit button" class="btn btn-default" value="Send">Send</button>
				</form>
			</div>
			<!-- Middle Block -->
			<div class="col-md-4">
				<h3><i class="fa fa-map-marker"></i> Contact Information</h3>
				<h4>Manager: Ron Warawa (780) 214-6244</h4>
				<p>For career opportunities, please visit <a href="#">this page</a>
			</div>
			<!-- CPDFN -->
			<div class="col-md-4">
			     <img src="/images/cpdfn.png" class="img-responsive img-center cpdfn-logo-sm">
			     <h4 class="text-center">TDN Contracting Ltd. is a wholly owned Chipewyan Prairie Dene First Nation company.</h4>
			</div>
		</div>
	</div>

	<div class="copyright">
		<div class="container">
			<div class="col-md-6">
				<p class="white-text">&copy; Copyright - <%= title %></p>
			</div>
			<div class="col-md-6">
				<a href="http://www.liquidtrends.com" target="_blank" class="copyright-link pull-right">Design and Development by Liquid Trends Media</p>
			</div>
		</div>
	</div>

</body>
